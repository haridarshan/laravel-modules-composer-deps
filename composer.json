{
  "name": "haridarshan/laravel-api-versioning",
  "description": "Manage module specific dependencies in Laravel using nWidart/laravel-modules",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Hari Darshan Gorana",
      "email": "er.haridarshan@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "nwidart/laravel-modules": "^10.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.6",
    "laravel/framework": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Haridarshan\\Laravel\\ApiVersioning\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nwidart\\Modules\\LaravelModulesServiceProvider"
      ],
      "dont-discover": [
        "Haridarshan\\Laravel\\ApiVersioning\\ApiVersionServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
